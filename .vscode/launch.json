{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Frontend debugger",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/src",
      "preLaunchTask": "ng serve",
      "sourceMaps": true,
      "skipFiles": ["node_modules/**/*.js"]
    }
  ]
}
